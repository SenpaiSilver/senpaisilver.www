{
    "files.eol": "\n",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
    },
    "files.trimTrailingWhitespace": true,

    "[markdown]": {
        "files.trimTrailingWhitespace": false,
        "editor.wordWrap": "on",
        "editor.quickSuggestions": false
    }
}